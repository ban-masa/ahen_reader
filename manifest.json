{
    "name": "AhenReader",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "Auto Horimiya Reader",
    "permissions": [
        "tabs",
        "http://dka-hero.com/top.html"
    ],
    "page_action": {
        "default_icon": "icon19.png",
        "default_title": "DokkaiAhen"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "icons": {
        "128": "icon128.png"
    }
}
